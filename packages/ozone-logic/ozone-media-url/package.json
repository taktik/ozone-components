{
  "name": "ozone-media-url",
  "version": "5.3.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Ozone helper to build ozone media url",
  "repository": {
    "type": "git",
    "url": "https://github.com/taktik/ozone-components/packages/ozone-logic/ozone-media-url"
  },
  "main": "dist/ozone-media-url.js",
  "types": "dist/ozone-media-url.d.ts",
  "scripts": {
    "prepublish": "npm run tsc",
    "tsc": "../../../node_modules/.bin/tsc",
    "test": "../../../node_modules/.bin/wct --skip-plugin=local --configFile wct.conf.js",
    "test:persist": "../../../node_modules/.bin/wct --skip-plugin=headless -p --configFile wct.conf.js",
    "build:watch": "../../../node_modules/.bin/webpack --config webpack.test.js --watch",
    "build": "../../../node_modules/.bin/webpack --config webpack.test.js",
    "pretest": "npm run build",
    "doc": "../../../node_modules/.bin/typedoc --excludeExternals --out  doc/ src/"
  },
  "author": "hj@taktik",
  "license": "ISC",
  "devDependencies": {
    "ozone-type": "^5.3.0"
  },
  "dependencies": {
    "ozone-api-item": "^5.3.1",
    "ozone-api-request": "^5.3.0",
    "ozone-config": "^5.3.0",
    "ozone-search-helper": "^5.3.0"
  }
}
