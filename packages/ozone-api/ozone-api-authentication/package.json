  "publishConfig": {
    "access": "public"
  },
  "description": "low level wrapper around ozone login, logout and authentication api",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run tslint && npm run compile",
    "compile": "../../../node_modules/.bin/tsc",
